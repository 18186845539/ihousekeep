<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@include file="/WEB-INF/html/base/baseurl.jsp"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>添加员工</title>
<link href="<%=baseurl%>/bootstrap/css/bootstrap.min.css"
	rel="stylesheet">
<script src="<%=baseurl%>/js/jquery.js"></script>
<script src="<%=baseurl%>/js/jquery.form.js"></script>
<script src="<%=baseurl%>/js/base.js"></script>
<script src="<%=baseurl%>/js/base_table.js"></script>
<script type="text/javascript"
	src="<%=baseurl%>/html/admin/page/page.js"></script>
<script src="<%=baseurl%>/bootstrap/js/bootstrap.min.js"></script>
<script type="text/javascript" src="<%=baseurl%>/js/up_down_load.js"></script>
<script type="text/javascript"
	src="<%=baseurl%>/html/main/riderelegant/detail_ridermanager.js"></script>
</head>
<body>

	<table border="1" class="table table-striped" style="width: 100%">
		<tr>
			<td>电话</td>
			<td><input readonly="readonly" id="phone" type="text" size="2" class="form-control"
				style="width: 30%"></td>
			<td>员工姓名</td>
			<td><input readonly="readonly" id="name" type="text" size="2" class="form-control"
				style="width: 30%"></td>
			
		</tr>
		<tr>
			<td>身份</td>
			<td><input readonly="readonly" id="level_name" type="text" size="2"
				class="form-control" style="width: 30%"></td>
			<td>上级身份</td>
			<td><input readonly="readonly" id="leader_name" type="text" size="2"
				class="form-control" style="width: 30%"></td>
		</tr>
		<tr>
			<td>员工图片</td>
			<td><img id="photo" src="" width='100' heigh='100'>
			<td></td>
			<td></td>
		<tr>

		</tr>
		<tr>
			<td>是否允许登录</td>
			<td><input readonly="readonly" id="is_login" type="text" size="2"
				class="form-control" style="width: 30%"></td>
			<td>位置</td>
			<td><input readonly="readonly" id="location" type="text" size="2"
				class="form-control" style="width: 30%"></td>
			<td></td>
		</tr>
		<tr>
			<td>接单状态</td>
			<td><input readonly="readonly" id="order_statue" type="text" size="2"
				class="form-control" style="width: 30%"></td>
			<td>得分</td>
			<td><input readonly="readonly" id="rider_code" type="text" size="2"
				class="form-control" style="width: 30%"></td>
			<td></td>
		</tr>
		<tr>
			<td>是否为优秀员工</td>
			<td><input readonly="readonly" id="is_excellent" type="text" size="2"
				class="form-control" style="width: 30%"></td>
			<td>服务订单数</td>
			<td><input readonly="readonly" id="rider_sum" type="text" size="2"
				class="form-control" style="width: 30%"></td>
			<td></td>
		</tr>
		<tr>
			<td >服务子项目</td>
			<td id="service_item" colspan="5"></td>
		</tr>


	</table>

</body>
</html>